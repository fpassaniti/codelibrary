    flex-direction: column;
    align-items: center;
    text-align: center;
    margin: 2rem;
}
.heading h1.title {
    font-size: 3em;
}
.heading h1 ~ .subtitle {
    font-size: 2em;
    font-weight: 200;
    margin: 0 1em 2em;
}
.heading h3.title {
    font-weight: 400;
}
.heading p {
